{
  "name": "pomodoro",
  "version": "0.2.0",
  "private": true,
  "homepage": "https://fcc-pomodoro-timer.surge.sh",
  "dependencies": {
    "@material-ui/core": "^1.5.0",
    "@material-ui/icons": "^2.0.2",
    "emotion": "^9.2.6",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
